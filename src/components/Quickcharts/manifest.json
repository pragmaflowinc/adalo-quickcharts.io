{
  "displayName": "Quickcharts",
  "defaultWidth": 360,
  "defaultHeight": 200,
  "resizeY": true,
  "components": "./quickChartsIo.js",
  "icon": "./example-thumbnail.png",
  

  "props": [    
    
    {
      "name": "listItems",
      "displayName": "Records to Use",
      "type": "list"      
    },

    {
      "name": "xValues",
      "displayName": "X Values",
      "type": "text",
      "role": "listItem",
      "reference": "listItems"      
    },


    {
      "name": "yValues",
      "displayName": "Y Values",
      "type": "text",      
      "role": "listItem",
      "reference": "listItems"   
    },

    {
      "name": "dataSetName",
      "displayName": "Data Set Name",
      "type": "text"
    },

    { "name": "onUrl",
      "displayName": "triggered when url is created",
      "type": "action",
      "arguments": [
        {
          "displayName": "The Url", 
          "type": "text"
        }
      ]
      
    }

    ]



}